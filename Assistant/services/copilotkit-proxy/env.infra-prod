# DrMigrate Production Infrastructure Configuration
# Domain: *.drmigrate.com
# Deployed to: production servers

# Server configuration
PORT=8001
NODE_ENV=production

# Python backend URL (AG-UI endpoint)
# Use 127.0.0.1 for local backend on same server
PYTHON_BACKEND_URL=http://127.0.0.1:8002/api

# CORS configuration
# Production infrastructure uses *.drmigrate.com domain
CORS_ORIGIN=*.drmigrate.com

# Observability
# Enable CopilotKit request/response logging and tracing
COPILOT_OBS_ENABLED=true

# Logging level
LOG_LEVEL=warn
