{
  "name": "drm-chat-copilotkit-server",
  "version": "1.0.0",
  "description": "Standalone CopilotKit proxy server for Dr. Migrate Chat",
  "packageManager": "pnpm@9.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "esbuild src/server.ts --bundle --platform=node --outfile=dist/server.js --packages=bundle --format=cjs",
    "build:tsc": "tsc",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "copilotkit",
    "proxy",
    "ag-ui",
    "pydantic-ai"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ag-ui/client": "0.0.40",
    "@copilotkit/runtime": "^1.10.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "esbuild": "^0.27.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
