# CopilotKit Proxy Server - Production Configuration

# Server configuration
PORT=8001
NODE_ENV=production

# Python backend URL (AG-UI endpoint)
# Use 127.0.0.1 for local backend on same server
PYTHON_BACKEND_URL=http://127.0.0.1:8002/api

# CORS configuration
# Production domain: *.drmigrate.com
# Dev domain: *.drmigrate.com.au
# Set to wildcard domain to support all subdomains (requests come through IIS reverse proxy)
CORS_ORIGIN=*.drmigrate.com.au

# Observability
# Enable CopilotKit request/response logging and tracing
COPILOT_OBS_ENABLED=true

# Logging level
LOG_LEVEL=info
